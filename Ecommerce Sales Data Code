      "text/plain": [
       "                        0      1\n",
       "0            treze tilias  19.09\n",
       "1                 indaial  16.73\n",
       "2     sao jose dos campos  17.08\n",
       "3               sao paulo  17.34\n",
       "4            porto alegre  17.62\n",
       "...                   ...    ...\n",
       "4105             guairaca  30.00\n",
       "4106             sambaiba  15.00\n",
       "4107           japaratuba  15.00\n",
       "4108               tuiuti  15.00\n",
       "4109         tibau do sul  15.00\n",
