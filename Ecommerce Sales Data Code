    "# Identify the correlation between product price and the number of times a product has been purchased.\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 77,
   "id": "fc56244c-b3d3-4298-bf53-2564ede6270f",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "array([[ 1.        , -0.10631514],\n",
       "       [-0.10631514,  1.        ]])"
      ]
     },
     "execution_count": 77,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "query = \"\"\"select count(order_items.order_item_id) order_count,\n",
    "round(avg(order_items.price),2) price,\n",
    "upper(products.product_category) category \n",
    "from order_items join products \n",
    "on order_items.product_id = products.product_id\n",
    "group by category \"\"\"\n",
    "\n",
    "cur.execute(query)\n",
    "data = cur.fetchall()\n",
    "df = pd.DataFrame(data,columns = [\"order count\",\"price\",\"category\"] )\n",
    "df\n",
    "\n",
    "arr1 = df[\"order count\"]\n",
    "arr2 = df[\"price\"]\n",
    "np.corrcoef([arr1,arr2])\n"
   ]
  },
